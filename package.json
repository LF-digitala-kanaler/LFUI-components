{
  "name": "lfui-components",
  "version": "7.1.0",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "4.3.1",
    "eonasdan-custom-bootstrap-datetimepicker-with-bootstrap-4": "^1.0.0",
    "jquery": "^3.4.1",
    "popper.js": "1.16.1",
    "vivus": "0.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-backgrounds": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-storyshots": "^6.2.9",
    "@storybook/addon-storyshots-puppeteer": "^6.2.9",
    "@storybook/addon-viewport": "^6.2.9",
    "@storybook/html": "^6.2.9",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.2",
    "corejs-typeahead": "^1.1.1",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "jest": "^26.6.3",
    "jquery-ui-bundle": "1.12.1",
    "mini-css-extract-plugin": "^1.3.7",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-base64": "^0.7.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-increase-specificity": "^0.6.0",
    "postcss-loader": "^3.0.0",
    "puppeteer": "^7.1.0",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.7",
    "sass-loader": "^10.1.1",
    "start-server-and-test": "^1.12.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-loader": "^6.2.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "start": "start-storybook -s ./src -p 6006",
    "build": "yarn build-lfui && build-storybook  -s src  -o public/$npm_package_version",
    "build-lfui": "webpack --mode production &&  rimraf dist/lfui/icons && cp -r src/icons dist/lfui/icons",
    "release": "auto changelog && npm run version && auto release",
    "test": "jest --no-cache",
    "test:update": "jest -u",
    "copy:icons": "rimraf src/icons && cp -r node_modules/lfui-icons/dist/sprite src/icons"
  },
  "jest": {
    "transform": {
      "^.+\\.stories\\.js?$": "@storybook/addon-storyshots/injectFileName",
      "^.+\\.js?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(bootstrap)/)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|html)$": "<rootDir>/src/test/__mocks__/fileMock.js",
      "\\.(css|scss|sass)$": "<rootDir>/src/test/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/.jest/register-context.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache",
      "public"
    ]
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 8",
    "safari >= 4"
  ]
}