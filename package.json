  "dependencies": {
    "bootstrap": "4.3.1",
    "eonasdan-custom-bootstrap-datetimepicker-with-bootstrap-4": "^1.0.0",
    "jquery": "^3.4.1",
    "lfui-icons": "git+https://24cd72455eb8aea8ea1c796c1421945df20fbe0f:x-oauth-basic@github.com/LF-digitala-kanaler/LFUI-icons.git",
    "popper.js": "1.14.7",
    "vivus": "0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@storybook/addon-a11y": "^6.1.14",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-backgrounds": "^6.1.14",
    "@storybook/addon-docs": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/addon-storyshots": "^6.1.14",
    "@storybook/addon-storyshots-puppeteer": "^6.1.14",
    "@storybook/addon-viewport": "^6.1.14",
    "@storybook/html": "^6.1.14",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "chartjs-chart-geo": "^2.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "corejs-typeahead": "^1.1.1",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "dedent": "0.6.0",
    "dsv-loader": "^2.0.0",
    "file-loader": "^3.0.1",
    "i": "^0.3.6",
    "install": "^0.12.2",
    "jest": "^25.1.0",
    "jquery-ui-bundle": "1.12.1",
    "mini-css-extract-plugin": "^1.3.5",
    "node-sass": "^4.12.0",
    "node-sass-import-once": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-base64": "^0.7.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-increase-specificity": "^0.6.0",
    "postcss-loader": "^3.0.0",
    "puppeteer": "^2.1.1",
    "react-test-renderer": "^16.11.0",
    "remove": "^0.1.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^7.1.0",
    "sass-module-importer": "^1.4.0",
    "start-server-and-test": "^1.10.8",
    "style-loader": "^0.23.1",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-loader": "^6.2.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "svg-sprite": "1.3.6",
    "webpack": "^4.32.0",
    "webpack-cli": "^3.3.2"
  },
  "scripts": {
    "start": "start-storybook -s ./public -p 6006 -s ./dist/lfui",
    "build": "build-storybook -c .storybook -s ./dist/lfui  .storybook -o public/$npm_package_version && webpack --mode production",
    "dev": "webpack --mode development",
    "release": "auto changelog && npm run version && auto release",
    "test": "jest --no-cache",
    "test:update": "jest -u",
    "copy:icons": "rimraf src/icons && cp -r node_modules/lfui-icons/dist/sprite src/icons"
  },
  "jest": {
    "transform": {
      "^.+\\.stories\\.js?$": "@storybook/addon-storyshots/injectFileName",
      "^.+\\.js?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(bootstrap)/)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|html)$": "<rootDir>/src/test/__mocks__/fileMock.js",
      "\\.(css|scss|sass)$": "<rootDir>/src/test/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/.jest/register-context.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache",
      "public"
    ]
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 8",
    "safari >= 4"
  ]
}
